{
  "name": "n-puzzle",
  "version": "0.0.1",
  "packageManager": "pnpm@10.8.0",
  "description": "N-Puzzle Game",
  "author": "Simon He",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simon-He95/n-puzzle.git"
  },
  "keywords": [
