    "@unocss/reset": "^0.29.6",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.24.0",
    "express": "^4.21.2",
    "jsdom": "^19.0.0",
    "typescript": "^4.9.5",
    "unocss": "^0.29.6",
    "unplugin-auto-import": "^0.6.9",
